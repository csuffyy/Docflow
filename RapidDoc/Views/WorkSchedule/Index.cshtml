@{
    ViewBag.Title = UIElementRes.UIElement.WorkSchedule;
}

<h2>@UIElementRes.UIElement.WorkSchedule</h2>

@{Html.RenderPartial("_NewEntityBtn");}

<div>
    @Html.Action("Grid")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/gridmvc")
}
@section jslaunch {
    grid_init("@Url.Action("GetWorkScheduleList", "WorkSchedule")")

    var detailsUrl = '@Url.Action("Calendar", "WorkSchedule")';
    pageGrids.ordersGrid.onRowSelect(function (e) {
    window.location.href = detailsUrl  + '/' + e.row.Id;
    });
}