    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/FileUpload/css/jquery.fileupload.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/FileUpload/css/jquery.fileupload-ui.css")" />
