    <meta name="author" content="Igor Dmitrov & Dmitriy Shkarendin" />
    <meta name="copyright" content="(c) DOCFLOW">
    <meta http-equiv="Reply-to" content="igdmitrov@gmail.com">

    <link rel="shortcut icon" href="@Url.ContentVersioned("~/Content/Custom/image-icon/favicon.ico")" type="image/x-icon" />
    <link rel="apple-touch-icon" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon.png")" />
    <link rel="apple-touch-icon" sizes="57x57" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-57x57.png")" />
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-72x72.png")" />
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-114x114.png")" />
    <link rel="apple-touch-icon" sizes="144x144" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-144x144.png")" />
    <link rel="apple-touch-icon" sizes="57x57" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-60x60.png")" />
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-120x120.png")" />
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-76x76.png")" />
    <link rel="apple-touch-icon" sizes="144x144" href="@Url.ContentVersioned("~/Content/Custom/image-icon/apple-touch-icon-152x152.png")" />
    <meta name="msapplication-TileImage" content="@Url.ContentVersioned("Content/Custom/image-icon/apple-touch-icon-144x144.png")" />
    <meta name="msapplication-TileColor" content="#2c3e50" />
    <meta name="msapplication-square70x70logo" content="@Url.ContentVersioned("Content/Custom/image-icon/smalltile.png")" />
    <meta name="msapplication-square150x150logo" content="@Url.ContentVersioned("Content/Custom/image-icon/mediumtile.png")" />
    <meta name="msapplication-wide310x150logo" content="@Url.ContentVersioned("Content/Custom/image-icon/widetile.png")" />
    <meta name="msapplication-square310x310logo" content="@Url.ContentVersioned("Content/Custom/image-icon/largetile.png")" />

    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/Bootstrap/css/bootstrap.min.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/Bootstrap/css/bootstrap-datepicker3.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/Bootstrap/css/bootstrap-datetimepicker.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/Bootstrap/css/bootstrap-timepicker.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/Bootstrap/css/bootstrap-duallistbox.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/Bootstrap/css/bootstrap-select.min.css")" />
    <link type="text/css" rel="stylesheet" href="@Url.ContentVersioned("~/Content/Font-awesome/css/font-awesome.min.css")" />
