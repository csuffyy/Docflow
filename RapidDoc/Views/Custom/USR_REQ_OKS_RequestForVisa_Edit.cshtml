@model RapidDoc.Models.ViewModels.USR_REQ_OKS_RequestForVisa_View
@using RapidDoc.Extensions;

<div class="editor-label">
    @Html.LabelForRequired(model => Model.Country)
</div>
<div class="editor-field">
    @Html.TextBoxFor(model => Model.Country, new { @class = "form-control", placeholder = "Страна следования", @autofocus = "required" })
</div>
<div class="editor-label">
    @Html.LabelForRequired(model => Model.FromDate)
</div>
<div class="editor-field">
    @Html.TextBoxFor(model => Model.FromDate, new { @class = "datepicker", placeholder = "С даты", startDate = "" })
</div>
<div class="editor-label">
    @Html.LabelForRequired(model => Model.ToDate)
</div>
<div class="editor-field">
    @Html.TextBoxFor(model => Model.ToDate, new { @class = "datepicker", placeholder = "До даты", startDate = "" })
</div>

@section jslaunch {
    datepicker_init("@UIElementRes.UIElement.LangName");
}