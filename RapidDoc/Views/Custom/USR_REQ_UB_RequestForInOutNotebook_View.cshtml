@model RapidDoc.Models.ViewModels.USR_REQ_UB_RequestForInOutNotebook_View
@using RapidDoc.Extensions;

<div class="row">
    <div class="col-xs-6">
        <div class="editor-label">
            @Html.LabelFor(model => Model.Organization)
        </div>
        <div class="editor-field">
            @Html.DisplayTextFor(model => Model.Organization)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => Model.Model)
        </div>
        <div class="editor-field">
            @Html.DisplayTextFor(model => Model.Model)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => Model.Name)
        </div>
        <div class="editor-field">
            @Html.DisplayTextFor(model => Model.Name)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => Model.ObjectAccess)
        </div>
        <div class="editor-field">
            @Html.EnumToDescription(Model.ObjectAccess)
        </div>
        <div class="editor-label">
            @Html.LabelFor(model => Model.Aim)
        </div>
        <div class="editor-field">
            @Html.DisplayTextFor(model => Model.Aim)
        </div>
    </div>
    <div class="col-xs-6">
        <h4 class="text-info">Срок действия</h4>
        <div class="row">
            <div class="col-md-6">
                <div class="editor-label">
                    @Html.LabelFor(model => Model.FromDate)
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => Model.FromDate, "MM/dd/yyyy")
                </div>
            </div>
            <div class="col-md-6">
                <div class="editor-label">
                    @Html.LabelFor(model => Model.ToDate)
                </div>
                <div class="editor-field">
                    @Html.DisplayFor(model => Model.ToDate, "MM/dd/yyyy")
                </div>
            </div>
        </div>
    </div>
</div>


