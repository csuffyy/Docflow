                            @Html.LabelRequired("AverageEarnings1")
                        </div>
                        <div class="editor-field input-group-sm">
                            @Html.TextArea("AverageEarnings1", null, new { @class = "form-control", @row = 2, placeholder = Html.DisplayName("AverageEarnings1") })
                        </div>
