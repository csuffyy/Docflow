    </ul>
    <div id="myTabContent" class="tab-content">
        <div role="tabpanel" class="tab-pane fade in active" id="home" aria-labelledby="home-tab">
            <div class="well">
                <div class="row">
                    <div class="row">
                        <div class="col-md-8">
                            <div class="editor-label">
                                @Html.LabelRequired("NameSettlView1")
                            </div>
                            <div class="editor-field input-group-sm">
                                @Html.TextBox("NameSettlView1", null, new { @class = "form-control", placeholder = Html.DisplayName("NameSettlView1") })
                            </div>
                        </div>
                        <div class="col-md-4">
                            <h4 class="text-info text-left">#1 Для бухгалтерского учета</h4>
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="editor-label">
                            @Html.LabelRequired("SettlView1")
                        </div>
                        <div class="editor-field">
                            @Html.EnumDropDownList("RapidDoc.Models.Repository.SettlView", "SettlView1", new { @class = "selectpickerfull" })
                        </div>
                        <div class="editor-label">
                            @Html.LabelRequired("SettlType1")
                        </div>
                        <div class="editor-field">
                            @Html.EnumDropDownList("RapidDoc.Models.Repository.SettlType", "SettlType1", new { @class = "selectpickerfull" })
                        </div>
                        @Html.Partial("_DocumentEditFields", Model, new ViewDataDictionary(ViewData) { { "FieldName", "TimesheetManagement1" } })
                    </div>
                    <div class="col-md-5">
                        <div class="editor-label">
                            @Html.LabelRequired("WaySettl1")
                        </div>
                        <div class="editor-field input-group-sm">
                            @Html.TextArea("WaySettl1", null, new { @class = "form-control", @row = 2, placeholder = Html.DisplayName("WaySettl1") })
                        </div>
                        <div class="editor-label">
                            @Html.LabelRequired("ReflectedBU1")
                        </div>
                        <div class="editor-field input-group-sm">
                            @Html.TextArea("ReflectedBU1", null, new { @class = "form-control", @row = 2, placeholder = Html.DisplayName("ReflectedBU1") })
                        </div>
                        <div class="editor-label">
                            @Html.LabelRequired("ReflectedDepartment1")
                        </div>
