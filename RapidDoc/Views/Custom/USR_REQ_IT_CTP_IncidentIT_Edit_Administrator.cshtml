@model RapidDoc.Models.ViewModels.USR_REQ_IT_CTP_IncidentIT_View
@using RapidDoc.Extensions;

@Html.HiddenFor(model => model.Id)
@Html.HiddenFor(model => model.DocumentTableId)
@Html.HiddenFor(model => model.Phone)
@Html.HiddenFor(model => model.RequestText)
@Html.HiddenFor(model => model.Users)
<hr>
<h3>Заполняет администратор HelpDesk</h3>
<div class="editor-label">
    @Html.LabelFor(model => model.ServiceName)
</div>
<div class="editor-field">
    @Html.DropDownList("ServiceName", (IEnumerable<SelectListItem>)ViewBag.ServiceIncidentList, new { @class = "selectpicker", @data_live_search = "true", @data_size = "10" })
    @Html.ValidationMessageFor(model => model.ServiceName)
</div>
<div class="editor-label">
    @Html.LabelForRequired(model => Model.ServiceIncidientPriority)
</div>
<div class="editor-field">
    @Html.EnumDropDownListFor(model => model.ServiceIncidientPriority, new { @class = "selectpicker" })
</div>
<div class="editor-label">
    @Html.LabelForRequired(model => Model.ServiceIncidientLevel)
</div>
<div class="editor-field">
    @Html.EnumDropDownListFor(model => model.ServiceIncidientLevel, new { @class = "selectpicker" })
</div>
<div class="editor-label">
    @Html.LabelForRequired(model => Model.ServiceIncidientLocation)
</div>
<div class="editor-field">
    @Html.EnumDropDownListFor(model => model.ServiceIncidientLocation, new { @class = "selectpicker" })
